
<h1><center>Program Listing<center></h1>

<p id="notice"><%= notice %></p>

<table>
<table border = "2" cellspacing="0" bgcolor="FFFFE0">
  <thead>
    <tr>
      <th>Program Name</th>
      <th>Program Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.program_name %></td>
        <td><%= program.program_notes %></td>
        <td><%= link_to 'Show', program %></td>
        <td><%= link_to 'Edit', edit_program_path(program) %></td>
        <td><%= link_to 'Destroy', program, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program', new_program_path %>
<%= link_to 'Project Dashboard', projects_url %>
<%= link_to 'People Page', people_url %>

